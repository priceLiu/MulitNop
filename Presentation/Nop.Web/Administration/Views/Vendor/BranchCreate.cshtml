@model VendorBranchModel
@{
    //page title
    ViewBag.Title = @T("Admin.Catalog.Vendors.Branches.AddNew").Text;
}
@using (Html.BeginForm())
{
    <div class="section-header">
        <div class="title">
        <img src="@Url.Content("~/Administration/Content/images/ico-customers.png")" alt="" />
            @T("Admin.Catalog.Vendors.Branches.AddNew") @Html.ActionLink("(" + T("Admin.Catalog.Vendors.Branches.BackToVendor") + ")", "Edit", new { id = Model.VendorId })
        </div>
        <div class="options">
            <input type="submit" value="@T("Admin.Common.Save")" class="t-button" />
        </div>
    </div>
    @Html.Partial("_CreateOrUpdateBranch", Model)
}