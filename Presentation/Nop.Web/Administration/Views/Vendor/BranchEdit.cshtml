@model VendorBranchModel
@{
    //page title
    ViewBag.Title = @T("Admin.Catalog.Vendors.Branches.EditBranch").Text;
}
@using (Html.BeginForm())
{
    <div class="section-header">
        <div class="title">
        <img src="@Url.Content("~/Administration/Content/images/ico-customers.png")" alt="" />
            @T("Admin.Catalog.Vendors.Branches.EditBranch") @Html.ActionLink("(" + T("Admin.Catalog.Vendors.Branches.BackToVendor") + ")", "Edit", new { id = Model.VendorId })
        </div>
        <div class="options">
            <button type="submit" name="save" value="save" class="t-button">@T("Admin.Common.Save")</button>
        </div>
    </div>
    @Html.Partial("_CreateOrUpdateBranch", Model)
}
